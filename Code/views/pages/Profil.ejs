<!DOCTYPE html>
<html>
    <head>
        <% include ../parties/head %>
        <link href="css/dashboard.css" rel="stylesheet">
        <meta name="keywords" content="Biere, Bière, rechercher, commander, alchool">
        <meta name="author" content="François Duchêne">
        <meta name="description" content="Catalogue complet de bières permettant de localiser des point de vente et de commander en ligne !">
        <title>BeerSearch</title>
    </head>
    <body>
        <nav class="navbar navbar-inverse navbar-fixed-top">
          <div class="container">
            <% include ../parties/nav %>
            <div id="navbar" class="collapse navbar-collapse">
              <ul class="nav navbar-nav">
                <li><a href="/RechercheBieres.html">Recherche bière</a></li>
                <li><a href="/PDV.html">Points de ventes</a></li>
                <li><a href="/Commander.html">Commander une bière</a></li>
                <li><a href="/contact.html">Contact</a></li>
              </ul>
            </div><!--/.nav-collapse -->
          </div>
        </nav>
        <main>
          <div class="container-fluid">
            <div class="row">
              <header class="col-sm-3 col-md-2 sidebar">
                <ul class="nav nav-sidebar">
                  <li class="active"><a href="#">Aperçu <span class="sr-only">(current)</span></a></li>
                  <li><a href="/outilAdmin.html">Outils d'administration</a></li>
                  <li><a href="#">Historique</a></li>
                </ul>
                <ul class="nav nav-sidebar">
                  <li><a href="/deconnexion.html">Se déconnecter</a></li>
                  <li><a href="/supprimerCompte.html">Supprimer son compte</a></li>
                </ul>
              </header>
              <section class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
                <h1 class="page-header">Profil</h1>
                <h2 class="sub-header">Vos coordonnées</h2>
                <section class="row placeholders">
                  <div class="col-sm-4 colonneProfil">
                    <h3>Coordonnées personnelles</h3>
                    <section class="row placeholder">
                      <div class="col-sm-6 texteProfil">
                        Pseudonyme:<br>
                        Nom: <br>
                        Prénom:<br>
                        Sexe:<br>
                        Téléphone:<br>
                        Adresse Mail:<br>
                      </div>
                      <div class="col-sm-6 donneeProfil">
                        <!-- A compléter -->
                        <%=utilisateur.pseudo %><br>
                        <%=utilisateur.name %><br>
                        <%=utilisateur.prenom %><br>
                        <%=utilisateur.sexe %><br>
                        <%=utilisateur.tel %><br>
                        <%=utilisateur.email %><br>
                      </div>
                    </section>
                  </div>
                </section>
                <section class="row placeholders">
                  <div class="col-sm-4 colonneProfil">
                    <h3>Facturation</h3>
                    <section class="row placeholder">
                      <div class="col-sm-6 texteProfil">
                        Adresse facturation:<br>
                        Code Postal:<br>
                        Localité:<br>
                        Pays:<br>
                        Compte de facturation:<br>
                        Solde disponible:<br>
                      </div>
                      <div class="col-sm-6 donneeProfil">
                        <!-- A compléter -->
                        <%=utilisateur.adress %><br>
                        <%=utilisateur.postalCode %><br>
                        <%=utilisateur.city %><br>
                        <%=utilisateur.country %><br>
                        <%=utilisateur.coordBank %><br>
                        <%=utilisateur.balance %><br>
                      </div>
                    </section>
                  </div>
                  <div class="col-sm-4">
                    <h3>Livraison</h3>
                    <section class="row placeholder">
                      <div class="col-sm-6 texteProfil">
                        Adresse livraison:<br>
                        Code Postal:<br>
                        Localité:<br>
                        Pays:<br>
                      </div>
                      <div class="col-sm-6 donneeProfil">
                        <!-- A compléter -->
                        <%=utilisateur.livrAdress %><br>
                        <%=utilisateur.livrPostalCode %><br>
                        <%=utilisateur.livrCity %><br>
                        <%=utilisateur.livrCountry %><br>
                      </div>
                    </section>
                  </div>
                </section>
              </section>
            </div>
          </div>
        </main>
        <footer class="footer">
          <% include ../parties/footer %>
        </footer>
          <% include ../parties/scripts %>
      </body>
</html>
